{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": ["device_id", "temperature", "humidity", "timestamp"],
    "properties": {
      "device_id": {
        "type": "string"
      },
      "temperature": {
        "type": "number",
        "minimum": 0,
        "maximum": 50
      },
      "humidity": {
        "type": "number",
        "minimum": 0,
        "maximum": 100
      },
      "timestamp": {
        "type": "number"
      }
    }
  }
  